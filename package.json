{
  "name": "test-ui",
  "version": "0.10.0",
  "description": "Test UI package",
  "private": true,
  "keywords": [
    "shadcn",
    "ui",
    "components",
    "registry",
    "synergy",
    "react",
    "tailwind"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./themes/default": "./dist/themes/default.css",
    "./themes/cyberpunk": "./dist/themes/cyberpunk.css",
    "./themes/retro-arcade": "./dist/themes/retro-arcade.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "registry:build": "mkdir -p public/r && shadcn build && npm run validate",
    "package:build": "pnpm tsup --config tsup.config.ts",
    "prepare": "pnpm package:build",
    "validate": "node scripts/validate-registry.js",
    "prepublishOnly": "npm run package:build && npm run validate",
    "publish:dry": "npm publish --dry-run",
    "release": "npm version patch && npm publish"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "tailwind-merge": "^3.3.1",
    "next": "15.4.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0"
  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "shadcn": "^2.9.2",
    "tailwindcss": "^4",
    "tsup": "^8.5.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
